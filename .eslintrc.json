{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "jest": true,
    "browser": true,
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-duplicate-imports": "error",
    "import/extensions": [
      "error",
      "always",
      {
        "js": "always"
      }
    ],
    "quotes": ["error", "double"]
  }
}
